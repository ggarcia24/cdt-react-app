{
  "consumer": {
    "name": "cdt-react-app"
  },
  "provider": {
    "name": "ExchangeRatesService"
  },
  "interactions": [
    {
      "description": "a query to fetch exchange rates",
      "providerState": "ExchangeRates are present",
      "request": {
        "method": "POST",
        "path": "/",
        "headers": {
          "content-type": "application/json"
        },
        "body": {
          "operationName": "GetExchangeRates",
          "query": "query GetExchangeRates {\n              rates(currency: \"USD\") {\n                currency\n                rate\n                __typename\n              }\n            }",
          "variables": {
            "foo": "bar"
          }
        },
        "matchingRules": {
          "$.body.query": {
            "match": "regex",
            "regex": "query\\s*GetExchangeRates\\s*\\{\\s*rates\\(currency:\\s*\"USD\"\\)\\s*\\{\\s*currency\\s*rate\\s*__typename\\s*\\}\\s*\\}"
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "data": {
            "rates": [
              {
                "currency": "ARS",
                "rate": "100.3054"
              }
            ]
          }
        },
        "matchingRules": {
          "$.body.data.rates": {
            "min": 1
          },
          "$.body.data.rates[*].*": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}